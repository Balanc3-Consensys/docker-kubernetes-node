{
    "name": "calico-k8s-network",
    "cniVersion": "0.3.0",
    "plugins": [
      {
        "type": "calico",
        "nodename": "${IP}",
        "etcd_endpoints": "${ETCD_ENDPOINTS}",
        "kubernetes": {
          "k8s_api_root": "${KUBERNETES_MASTER}"
        },
        "log_level": "info",
        "ipam": {
            "type": "calico-ipam"
        }
      },
      {
        "type": "portmap",
        "capabilities": {"portMappings": true},
        "noSnat": true
      }
    ]
}
